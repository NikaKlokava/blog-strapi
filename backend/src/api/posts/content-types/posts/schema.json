{
  "kind": "collectionType",
  "collectionName": "all_posts",
  "info": {
    "singularName": "posts",
    "pluralName": "all-posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "best_status": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "section": {
      "type": "string",
      "required": true
    },
    "post_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-content.post-content",
      "mappedBy": "post"
    }
  }
}
